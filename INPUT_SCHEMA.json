{
  "title": "Lectio - Input schema for the apify actor",
  "description": "",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "school_id": {
      "title": "School ID (from url)",
      "type": "integer",
      "description": "School ID",
      "editor": "number"
    },
    "username": {
      "title": "Username",
      "type": "string",
      "description": "Username to log in",
      "editor": "textfield"
    },
    "password": {
      "title": "Password",
      "type": "string",
      "description": "Password to log in",
      "editor": "hidden"
    },
    "weekcount": {
      "title": "Week count",
      "type": "integer",
      "description": "Number of weeks to scrape",
      "default": 8,
      "editor": "number",
      "maximum": 30,
      "minimum": 1
    }
  },
  "required": [
    "school_id",
    "username",
    "password",
    "weekcount"
  ]
}
